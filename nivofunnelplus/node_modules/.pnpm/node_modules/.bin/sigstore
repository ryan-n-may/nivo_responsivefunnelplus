#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ryanmay/Documents/GitHub/nivo_responsivefunnelplus/nivofunnelplus/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/ryanmay/Documents/GitHub/nivo_responsivefunnelplus/nivofunnelplus/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sigstore/bin/sigstore.js" "$@"
else
  exec node  "$basedir/../sigstore/bin/sigstore.js" "$@"
fi
